<div class="span1 btn new-user" data-original-title="" title="">
  <%= link_to "New User", new_admin_user_path %>
</div>

<% @users.each do |user|%>
  <div class="container-fluid">
    <div class="row-fluid">
      <div class="span12">
        <div class="span8 offset2">
          <div class="row-fluid client-cards">
            <div class="span10">
              <div class="row-fluid">
                <h2><%= user.username %></h2>
              </div>
              <div class="row-fluid">
                <div class="span11">
                  <p><%= user.description %></p>
                </div>
              </div>
              <div class="row-fluid">
                <a href="" class="btn">UPLOAD REPORT</a>
                <a href="" class="btn">IMPERSONATE</a>
                <span class="btn"><%= link_to "INVITE USER", new_invitation_path(:user) %></span>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
<%end%>
